{
	"$schema": "https://raw.githubusercontent.com/Mearman/SPIRAL/main/air.schema.json",
	"version": "1.0.0",
	"description": "Construct a list from computed node values and get its length",
	"airDefs": [],
	"nodes": [
		{
			"id": "a",
			"expr": { "kind": "lit", "type": { "kind": "int" }, "value": 10 }
		},
		{
			"id": "b",
			"expr": { "kind": "lit", "type": { "kind": "int" }, "value": 20 }
		},
		{
			"id": "c",
			"expr": { "kind": "call", "ns": "core", "name": "add", "args": ["a", "b"] }
		},
		{
			"id": "myList",
			"expr": {
				"kind": "listOf",
				"elements": ["a", "b", "c"]
			}
		},
		{
			"id": "result",
			"expr": { "kind": "call", "ns": "list", "name": "length", "args": ["myList"] }
		}
	],
	"result": "result",
	"expected_result": 3
}
