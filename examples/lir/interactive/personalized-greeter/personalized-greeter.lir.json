{
	"$schema": "https://raw.githubusercontent.com/Mearman/SPIRAL/main/lir.schema.json",
	"version": "1.0.0",
	"description": "LIR representation of personalized-greeter: demonstrates mixed-cadence I/O pattern in straight-line form with print effect instructions.",
	"nodes": [
		{
			"id": "main",
			"blocks": [
				{
					"id": "entry",
					"instructions": [
						{
							"kind": "assign",
							"target": "welcome_msg",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "Welcome!"
							}
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out1",
							"args": [
								"welcome_msg"
							]
						},
						{
							"kind": "assign",
							"target": "name_prompt",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "What is your name?"
							}
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out2",
							"args": [
								"name_prompt"
							]
						},
						{
							"kind": "assign",
							"target": "name",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "Alice"
							}
						},
						{
							"kind": "assign",
							"target": "hello_prefix",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "Hello, "
							}
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out3",
							"args": [
								"hello_prefix"
							]
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out4",
							"args": [
								"name"
							]
						},
						{
							"kind": "assign",
							"target": "age_prompt",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "What is your age?"
							}
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out5",
							"args": [
								"age_prompt"
							]
						},
						{
							"kind": "assign",
							"target": "age",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "int"
								},
								"value": 25
							}
						},
						{
							"kind": "assign",
							"target": "age_prefix",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "You are "
							}
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out6",
							"args": [
								"age_prefix"
							]
						},
						{
							"kind": "effect",
							"op": "printInt",
							"target": "out7",
							"args": [
								"age"
							]
						},
						{
							"kind": "assign",
							"target": "years_msg",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "years old."
							}
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out8",
							"args": [
								"years_msg"
							]
						},
						{
							"kind": "assign",
							"target": "goodbye_msg",
							"value": {
								"kind": "lit",
								"type": {
									"kind": "string"
								},
								"value": "Goodbye, "
							}
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out9",
							"args": [
								"goodbye_msg"
							]
						},
						{
							"kind": "effect",
							"op": "print",
							"target": "out10",
							"args": [
								"name"
							]
						}
					],
					"terminator": {
						"kind": "return"
					}
				}
			],
			"entry": "entry"
		}
	],
	"result": "main",
	"expected_output": [
		"Welcome!",
		"What is your name?",
		"Hello, ",
		"Alice",
		"What is your age?",
		"You are ",
		"25",
		"years old.",
		"Goodbye, ",
		"Alice"
	],
	"expected_result": null,
	"note": "LIR representation of personalized-greeter using a single straight-line block. Uses hardcoded values (Alice, 25) for name and age rather than actual I/O effects. Demonstrates mixed-cadence output pattern with sequential effects and value reuse within a block."
}
